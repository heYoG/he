<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<!-- javabean注入 -->
<beans>
	<!-- action中的javabean调用依赖注入-配置文件方式 -->
	<bean id="SIVo" class="vo.sealImageVo.SealImageVo"></bean>
	<bean id="commenClass" class="util.CommenClass"></bean>
	<!-- 测试aop类 -->
	<!-- <bean id="aspector" class="test.aop.AspectClass"></bean> 切面 -->
	<!-- <bean id="pointcut" class="test.aop.PointcutClass"></bean>切入点 -->
	<!-- aop实际使用类(配置方式) -->
	<bean id="log" class="vo.logVo.LogVo"></bean>
	
	<!-- spring容器管理Axis2的Web Service -->
	<bean id="webService" class="webservice.WebServiceInterface"></bean>
	<!-- 声明axis2由spring管理，必须 -->
	<bean class="org.apache.axis2.extensions.spring.receivers.ApplicationContextHolder"></bean>
</beans>