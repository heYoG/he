<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans >
		
		<form-bean name="hotelTDicForm" type="com.dj.seal.hotel.form.HotelTDicForm">
		</form-bean>
		<form-bean name="masterplatejudgeForm" type="com.dj.seal.hotel.form.MasterplatejudgeForm"></form-bean>
		<form-bean name="hotelAdvertForm" type="com.dj.seal.hotel.form.HotelAdvertForm"></form-bean>
	</form-beans>
	
	<action-mappings>
		<action path="/newDic" 
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelTDicForm">
			<forward name="success"
				path="/general/hotel/dictionary/new_dic.jsp">
			</forward>
		</action>
		<action path="/newsAdvert" 
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelAdvertForm">
			<forward name="success" path="/general/hotel/advert/ad_list_history.jsp">
			</forward>
		</action>
		<action path="/updAdvert" 
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelAdvertForm">
			<forward name="success" path="/general/hotel/advert/ad_list.jsp">
			</forward>
		</action>
		<action path="/newjud"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="masterplatejudgeForm">
			<forward name="success"
				path="/general/hotel/judge/jud_list.jsp">
			</forward>
		</action>
		
		<action path="/recordDownload" type="org.springframework.web.struts.DelegatingActionProxy"></action>
		<action path="/newAdvert"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelAdvertForm">
			<forward name="success"
				path="/general/hotel/advert/index.jsp">
			</forward>
		</action>
		<action path="/adApps"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelAdvertForm">
			<forward name="success"
				path="/general/hotel/advert/approve_list.jsp?ad_state=0">
			</forward>
		</action>
		<action path="/appAdvert"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="hotelAdvertForm">
			<forward name="success"
				path="/general/hotel/advert/ad_list_history.jsp">
			</forward>
		</action>
		
	</action-mappings>
	
</struts-config>

