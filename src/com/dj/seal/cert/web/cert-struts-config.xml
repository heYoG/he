<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="certForm"
			type="com.dj.seal.cert.web.form.CertForm">
		</form-bean>
	</form-beans>

	<action-mappings>
		<action path="/certAdd"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="certForm">
			<forward name="success"
				path="/general/cert/cert_reg/cert_list.jsp">
			</forward>
		</action>
		<action path="/certUpd"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="certForm">
			<forward name="success"
				path="/general/cert/cert_mng/cert_list.jsp">
			</forward>
		</action>
	</action-mappings>

</struts-config>

