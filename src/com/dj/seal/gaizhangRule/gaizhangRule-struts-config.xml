<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="gaiZhangRuleForm"
			type="com.dj.seal.gaizhangRule.form.GaiZhangRuleForm">
		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/newRule"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="gaiZhangRuleForm">
			<forward name="success"
				path="/general/gaizhangRule/rule_mng/rule_list.jsp">
			</forward>
		</action>
	</action-mappings>
</struts-config>

