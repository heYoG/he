<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans >
		
		<form-bean name="modelFileRuleForm" type="com.dj.seal.modelFileRule.form.ModelFileRuleForm"></form-bean>
	</form-beans>
	
	<action-mappings>
		<action path="/newMFR"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" name="modelFileRuleForm">
			<forward name="success"
				path="/general/modelFileRule/modelFileRule_list.jsp">
			</forward>
		</action>
		
	</action-mappings>
	
</struts-config>

